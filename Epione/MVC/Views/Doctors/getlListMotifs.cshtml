
@{
    ViewBag.Title = "getlListMotifs";
    Layout = "~/Views/Shared/_DoctorsLayout.cshtml";
    
}
@Styles.Render("~/motifsDoctor/css");
<div class="container mesMotifs">
    <div class="row">
        <div class="col-lg-12 title"><h2>Mes motifs</h2></div>
    </div>
    <div class="row">
        @foreach(dynamic maMotif in ViewBag.mesMotifs)
        {
            <div class="col-lg-6"> + @maMotif.description</div>
        }
    </div>
</div>

<div class="container allMotifs">
    <div class="row">
        <div class="col-lg-12 title"><h2>List des motifs</h2></div>
    </div>
    <div class="row">
      
            @foreach (dynamic motifs in ViewBag.allMotifs)
            {
                
                <div class="col-lg-6">@motifs.description</div>
            }
        @using (Html.BeginForm("test", "Doctors"))
        {
            List<int> list = new List<int>();
            list.Add(1);
            list.Add(2);
            <input type="submit" value="submit" />
        }
            
     
            
    </div>
</div>


